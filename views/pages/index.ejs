<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('../partials/head') %>
</head>

<body>
  <nav>
    <%- include('../partials/navbar') %>
  </nav>
  <main>
    <h4 id="dbResultTable">Search Results</h4>
    <%= dbResults.title %>
    <% dbResults.forEach(book => { %>
      <div class="booksDiv">
        <p><img src="<%= book.image_url %>"></p>
        <p>
          <%= book.title %>
        </p>
        <p>
          <%= book.author %>
        </p>
        <p class="description">
          <%= book.description %>
        </p>
      </div>
      <% }); %>
  </main>
</body>

</html>